{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/background_music.mp3",
      "headers": {
        "Cache-Control": "max-age=31536000",
        "Content-Type": "audio/mpeg"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}